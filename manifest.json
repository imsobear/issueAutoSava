{
    "name": "Github Issue AutoSave",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "自动保存issue里的内容。",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title":"自动保存issue里的内容",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://github.com/*"
    ],
    "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["css/content.css"],
      "js": ["js/content.js"]
    }
    ]
}
